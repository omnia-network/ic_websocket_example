{
  "name": "ic_websocket_example",
  "version": "0.3.0",
  "description": "Internet Computer starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "build": "cd src/ic_websocket_example_frontend && npm run build",
    "prebuild": "npm run generate",
    "start": "cd src/ic_websocket_example_frontend && npm run dev",
    "prestart": "npm run generate",
    "deploy:local": "dfx deploy --network=local",
    "deploy:ic": "dfx deploy --network=ic",
    "generate": "dfx generate ic_websocket_example_backend"
  },
  "dependencies": {
    "@dfinity/agent": "^2.0.0",
    "@dfinity/candid": "^2.0.0",
    "@dfinity/identity-secp256k1": "^2.0.0",
    "@dfinity/principal": "^2.0.0",
    "ic-websocket-js": "^0.4.0"
  },
  "devDependencies": {
    "dotenv": "^16.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ]
}
